{
  "compilerOptions": {
    "declaration": true,
    "target": "ES6",
    "module": "ES6",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "preserveSymlinks": true,
    "outDir": "./dist",
    "exactOptionalPropertyTypes": true
  },
  "include": ["./src"],
  "exclude": ["node_modules/"]
}
